	//int a,a;
	int idade, cont;
	char casa;
	float pi;
	
	// variavel nao declarada
	//ola = 10;

	// tipo incompativel de atribuicao
//	pi = 'b';
	
	idade = 3;
	cont = 1;
	casa = 'a';
	pi = 0.5;
	
	if (pi < casa)
		idade = 4;
	pi = idade + cont;
	do{
		int idade, cont;
		char casa;
		float pi, pa;
		//multipla definicao para tipo diferente, em linha diferente
		//char casa;
				
		idade = 10;
		cont = 2;
		casa = 'b';

		pi = 1.0;
		pa = 1.0;
		
		
		while(cont < idade){
			int idade, cont;
			char casa;
			
			idade = 15;
			cont = 3;
			casa = 'c';	
			if (idade < cont)
				idade = 10;
		}

	}while(idade > cont);


}
