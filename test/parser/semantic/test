int main(){
	int a, b, c, d, e, x;
	//a = b *  10 * 5 / 3 * 2;
	//b = 2 + 3 - 10 + 2;
	//a = (2 + 3) / 4;
	//a = (1 * 2 + 3 * 4 * 5 + 10 - 4 / 2 + 9);
	//b = a;
	//a = a / b * (c / d) + e;
		
	if (a > b + x){
 		x = x + 1;
		if (a < b)
			a = 2;
	}
	else
 		x = x - 1;

}

